background no
out_to_console yes
out_to_x no
update_interval 1.0
total_run_times 0
use_spacer none
times_in_seconds true

TEXT
^fg(\#FF0000) ਃ ^fg()${execi 6 mpc current} \
${execi 6 python2 ~/.dotfiles/message_indicator.py}\
^fg(\#A6E22E) ਁ ^fg()${exec ~/Code/Bash/volume.sh}\
^fg(\#AE81FF)\
${if_match "$nodename" == "psirus-laptop"}\
${if_match ${battery_percent BAT1} <= 100}${if_match ${battery_percent BAT1} > 85} ^i(/home/psirus/.dotfiles/icons/battery90.xbm) ${endif}${endif}\
${if_match ${battery_percent BAT1} <= 85}${if_match ${battery_percent BAT1} > 75} ^i(/home/psirus/.dotfiles/icons/battery80.xbm) ${endif}${endif}\
${if_match ${battery_percent BAT1} <= 75}${if_match ${battery_percent BAT1} > 65} ^i(/home/psirus/.dotfiles/icons/battery70.xbm) ${endif}${endif}\
${if_match ${battery_percent BAT1} <= 65}${if_match ${battery_percent BAT1} > 55} ^i(/home/psirus/.dotfiles/icons/battery60.xbm) ${endif}${endif}\
${if_match ${battery_percent BAT1} <= 55}${if_match ${battery_percent BAT1} > 45} ^i(/home/psirus/.dotfiles/icons/battery50.xbm) ${endif}${endif}\
${if_match ${battery_percent BAT1} <= 45}${if_match ${battery_percent BAT1} > 35} ^i(/home/psirus/.dotfiles/icons/battery40.xbm) ${endif}${endif}\
${if_match ${battery_percent BAT1} <= 35}${if_match ${battery_percent BAT1} > 25} ^i(/home/psirus/.dotfiles/icons/battery30.xbm) ${endif}${endif}\
${if_match ${battery_percent BAT1} <= 25}${if_match ${battery_percent BAT1} > 15} ^i(/home/psirus/.dotfiles/icons/battery20.xbm) ${endif}${endif}\
${if_match ${battery_percent BAT1} <= 15}^i(/home/psirus/.dotfiles/icons/battery10.xbm)${endif}^fg()\
${if_match "${battery_short BAT1}" == "U"}Full ${else} ${if_match "${battery_short BAT1}" == "F"}Full ${else}${format_time ${battery_time BAT1} "\hh\mm"} ${endif}${endif}\
${endif}\
^fg(\#FD971F) ਂ ^fg() ${time %a.%d - %H:%M} 
